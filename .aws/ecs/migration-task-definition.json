{
  "executionRoleArn": "arn:aws:iam::SED_TARGET_AWS_ACCOUNT_ID:role/Sample-TaskExecution",
  "taskRoleArn": "arn:aws:iam::SED_TARGET_AWS_ACCOUNT_ID:role/Sample-TaskExecution",
  "family": "Sample",
  "cpu": "256",
  "memory": "1024",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "SED_TARGET_AWS_ACCOUNT_ID.dkr.ecr.ap-northeast-1.amazonaws.com/Sample-app:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "ap-northeast-1",
          "awslogs-group": "/Sample/ecs",
          "awslogs-stream-prefix": "Sample-app-migration"
        }
      },
      "environment": [
        {
          "name": "LOG_CHANNEL",
          "value": "stderr"
        },
        {
          "name": "DB_CONNECTION",
          "value": "pgsql"
        }
      ],
      "secrets": [
        {
          "name": "APP_KEY",
          "valueFrom": "APP_KEY"
        },
        {
          "name": "DB_HOST",
          "valueFrom": "DB_HOST"
        },
        {
          "name": "DB_DATABASE",
          "valueFrom": "DB_DATABASE"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "DB_USERNAME"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "DB_PASSWORD"
        }
      ],
      "entryPoint": [
        "php",
        "artisan",
        "migrate:refresh",
        "--seed",
        "--force"
      ]
    }
  ]
}